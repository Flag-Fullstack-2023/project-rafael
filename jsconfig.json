{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "_components/*": ["./src/components/*"],
      "_atoms/*": ["src/components/_atoms"],
      "_molecules/*": ["src/components/_molecules"],
      "_organisms/*": ["src/components/_organisms"],
      "_sections/*": ["./src/components/sections/"],
      "_pages/*": ["src/_pages"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
